{
  "kind": "collectionType",
  "collectionName": "case_studies",
  "info": {
    "singularName": "case-study",
    "pluralName": "case-studies",
    "displayName": "case-study"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true,
      "unique": false
    },
    "description": {
      "type": "text",
      "required": true
    },
    "case_study_categories": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::case-study-category.case-study-category"
    },
    "active_status": {
      "type": "boolean",
      "required": true,
      "default": true
    },
    "image": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images",
        "files"
      ]
    },
    "slug": {
      "type": "uid",
      "targetField": "title",
      "required": true
    }
  }
}
